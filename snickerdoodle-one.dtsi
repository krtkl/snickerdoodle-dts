/include/ "snickerdoodle.dtsi"
/ {
        model = "snickerdoodle one";
        compatible = "xlnx,zynq-snickerdoodle-one", "xlnx,zynq-snickerdoodle", "xlnx,zynq-7000";

	memory@0 {
		device_type = "memory";
		reg = <0x0 0x20000000>;
	};
};

&wlcore {
	compatible = "wlcore", "ti,wl1801";
};
